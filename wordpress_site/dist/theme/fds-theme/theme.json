{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true,
    "color": {
      "palette": [
        {
          "slug": "primary",
          "color": "#0B3D91",
          "name": "Primary Blue"
        },
        {
          "slug": "dark",
          "color": "#0A1A2F",
          "name": "Dark Navy"
        },
        {
          "slug": "accent",
          "color": "#12B886",
          "name": "Success Green"
        },
        {
          "slug": "neutral-900",
          "color": "#111827",
          "name": "Neutral 900"
        },
        {
          "slug": "neutral-500",
          "color": "#6B7280",
          "name": "Neutral 500"
        },
        {
          "slug": "neutral-200",
          "color": "#E5E7EB",
          "name": "Neutral 200"
        },
        {
          "slug": "neutral-50",
          "color": "#F9FAFB",
          "name": "Neutral 50"
        },
        {
          "slug": "white",
          "color": "#FFFFFF",
          "name": "White"
        }
      ],
      "gradients": [
        {
          "slug": "primary-gradient",
          "gradient": "linear-gradient(135deg, #0B3D91 0%, #0A1A2F 100%)",
          "name": "Primary Gradient"
        },
        {
          "slug": "accent-gradient",
          "gradient": "linear-gradient(135deg, #12B886 0%, #0B3D91 100%)",
          "name": "Accent Gradient"
        }
      ]
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "Inter, system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
          "slug": "inter",
          "name": "Inter"
        },
        {
          "fontFamily": "'Work Sans', Inter, system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
          "slug": "work-sans",
          "name": "Work Sans"
        }
      ],
      "fontSizes": [
        {
          "slug": "small",
          "size": "0.875rem",
          "name": "Small"
        },
        {
          "slug": "medium",
          "size": "1rem",
          "name": "Medium"
        },
        {
          "slug": "large",
          "size": "1.125rem",
          "name": "Large"
        },
        {
          "slug": "x-large",
          "size": "1.25rem",
          "name": "Extra Large"
        },
        {
          "slug": "xx-large",
          "size": "1.5rem",
          "name": "2X Large"
        },
        {
          "slug": "xxx-large",
          "size": "2rem",
          "name": "3X Large"
        },
        {
          "slug": "huge",
          "size": "2.5rem",
          "name": "Huge"
        },
        {
          "slug": "gigantic",
          "size": "3rem",
          "name": "Gigantic"
        }
      ]
    },
    "spacing": {
      "spacingScale": {
        "steps": 0
      },
      "spacingSizes": [
        {
          "size": "0.5rem",
          "slug": "20",
          "name": "XS"
        },
        {
          "size": "1rem",
          "slug": "30",
          "name": "S"
        },
        {
          "size": "1.5rem",
          "slug": "40",
          "name": "M"
        },
        {
          "size": "2rem",
          "slug": "50",
          "name": "L"
        },
        {
          "size": "3rem",
          "slug": "60",
          "name": "XL"
        },
        {
          "size": "4rem",
          "slug": "70",
          "name": "XXL"
        },
        {
          "size": "6rem",
          "slug": "80",
          "name": "3XL"
        }
      ]
    },
    "layout": {
      "contentSize": "1200px",
      "wideSize": "1400px"
    },
    "border": {
      "radius": true,
      "color": true,
      "style": true,
      "width": true
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--white)",
      "text": "var(--wp--preset--color--neutral-900)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--inter)",
      "fontSize": "var(--wp--preset--font-size--medium)",
      "lineHeight": "1.6"
    },
    "spacing": {
      "blockGap": "1.5rem"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--work-sans)",
          "fontSize": "var(--wp--preset--font-size--gigantic)",
          "fontWeight": "800",
          "lineHeight": "1.2"
        },
        "color": {
          "text": "var(--wp--preset--color--neutral-900)"
        },
        "spacing": {
          "margin": {
            "bottom": "1rem"
          }
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--work-sans)",
          "fontSize": "var(--wp--preset--font-size--huge)",
          "fontWeight": "700",
          "lineHeight": "1.3"
        },
        "color": {
          "text": "var(--wp--preset--color--neutral-900)"
        },
        "spacing": {
          "margin": {
            "bottom": "1rem"
          }
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--work-sans)",
          "fontSize": "var(--wp--preset--font-size--xxx-large)",
          "fontWeight": "700",
          "lineHeight": "1.4"
        },
        "color": {
          "text": "var(--wp--preset--color--neutral-900)"
        },
        "spacing": {
          "margin": {
            "bottom": "0.75rem"
          }
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--work-sans)",
          "fontSize": "var(--wp--preset--font-size--xx-large)",
          "fontWeight": "700",
          "lineHeight": "1.4"
        },
        "color": {
          "text": "var(--wp--preset--color--neutral-900)"
        }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--work-sans)",
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "fontWeight": "600",
          "lineHeight": "1.4"
        },
        "color": {
          "text": "var(--wp--preset--color--neutral-900)"
        }
      },
      "h6": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--work-sans)",
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "600",
          "lineHeight": "1.4"
        },
        "color": {
          "text": "var(--wp--preset--color--neutral-500)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--primary)"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--accent)"
          }
        }
      },
      "button": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--inter)",
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "500"
        },
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--preset--color--white)"
        },
        "border": {
          "radius": "0.75rem"
        },
        "spacing": {
          "padding": {
            "top": "0.75rem",
            "bottom": "0.75rem",
            "left": "1.5rem",
            "right": "1.5rem"
          }
        },
        ":hover": {
          "color": {
            "background": "var(--wp--preset--color--dark)"
          }
        }
      }
    },
    "blocks": {
      "core/paragraph": {
        "typography": {
          "lineHeight": "1.7"
        },
        "spacing": {
          "margin": {
            "bottom": "1rem"
          }
        }
      },
      "core/list": {
        "spacing": {
          "margin": {
            "bottom": "1rem"
          }
        }
      },
      "core/quote": {
        "border": {
          "left": {
            "width": "4px",
            "style": "solid",
            "color": "var(--wp--preset--color--accent)"
          }
        },
        "spacing": {
          "padding": {
            "left": "1.5rem"
          }
        },
        "typography": {
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontStyle": "italic"
        }
      },
      "core/separator": {
        "color": {
          "background": "var(--wp--preset--color--neutral-200)"
        },
        "spacing": {
          "margin": {
            "top": "2rem",
            "bottom": "2rem"
          }
        }
      }
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    }
  ],
  "customTemplates": [
    {
      "name": "front-page",
      "title": "Front Page"
    },
    {
      "name": "page-services",
      "title": "Services Page"
    },
    {
      "name": "page-contact",
      "title": "Contact Page"
    },
    {
      "name": "single-case_study",
      "title": "Case Study"
    },
    {
      "name": "archive-case_study",
      "title": "Case Studies"
    }
  ]
}